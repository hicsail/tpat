}, 301000);\n    }\n    if (recordWebcam.status === CAMERA_STATUS.PREVIEW) {\n      setTimeout(() => {\n        navigate(\"/\");\n        //1 second = 1000 millisecond\n      }, 1000);\n      recordWebcam.close();\n      recordWebcam.download();\n    }\n  });\n\n  return (\n    <div style={{ marginLeft: \"5%\", paddingTop: \"3%\", marginRight: \"5%\" }}>\n      <p>Camera status: {recordWebcam.status}</p>\n      {recordWebcam.status === CAMERA_STATUS.OPEN ||\n      recordWebcam.status === CAMERA_STATUS.RECORDING ? (\n        <CountDownTimer hoursMinSecs={hoursMinSecs} />\n      ) : (\n        <p>Time Limit: 5 minutes</p>\n      )}\n\n      <div class=\"webcam\">\n        <button\n          disabled={\n            recordWebcam.status === CAMERA_STATUS.OPEN ||\n            recordWebcam.status === CAMERA_STATUS.RECORDING ||\n            recordWebcam.status === CAMERA_STATUS.PREVIEW\n          }\n          onClick={() => {\n            recordWebcam.start();\n            recordWebcam.open();\n          }}\n        >\n          Open camera\n        </button>\n        <button\n          disabled={\n            recordWebcam.status === CAMERA_STATUS.CLOSED ||\n            recordWebcam.status === CAMERA_STATUS.RECORDING ||\n            recordWebcam.status === CAMERA_STATUS.PREVIEW\n          }\n          onClick={() => {\n            recordWebcam.start();\n            setRecord(!isRecord);\n          }}\n        >\n          Start recording\n        </button>\n        <button\n          disabled={recordWebcam.status !== CAMERA_STATUS.RECORDING}\n          onClick={() => {\n            recordWebcam.stop();\n            setRecord(!isRecord);\n          }}\n        >\n          Stop recording\n        </button>\n      </div>\n      <WaterMark content={content}>\n        <video\n          ref={recordWebcam.webcamRef}\n          style={{\n            display: `${\n              recordWebcam.status === CAMERA_STATUS.OPEN ||\n              recordWebcam.status === CAMERA_STATUS.RECORDING\n                ? \"block\"\n                : \"none\"\n            }`,\n          }}\n          autoPlay\n          muted\n        />\n      </WaterMark>\n      <video\n        ref={recordWebcam.previewRef}\n        style={{\n          display: `${\n            recordWebcam.status === CAMERA_STATUS.PREVIEW ? \"block\" : \"none\"\n          }`,\n        }}\n        controls\n      />\n    </div>\n  );\n}\n",[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/pages/home.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/components/Timer.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/components/Btn.js",[],[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/components/Card.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/pages/task.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/components/Footer.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/pages/contact.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/pages/instruction.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/pages/tutorial.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/pages/tutorial2.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/components/Alert.js",[],[],"/Users/so._.may._.see._.macy/Desktop/tpat/src/data.js",["120"],[],{"ruleId":"121","severity":1,"message":"122","line":23,"column":9,"nodeType":"123","messageId":"124","endLine":23,"endColumn":23},{"ruleId":"121","severity":1,"message":"125","line":25,"column":9,"nodeType":"123","messageId":"124","endLine":25,"endColumn":13},{"ruleId":"121","severity":1,"message":"126","line":27,"column":9,"nodeType":"123","messageId":"124","endLine":27,"endColumn":16},{"ruleId":"121","severity":1,"message":"127","line":1,"column":8,"nodeType":"123","messageId":"124","endLine":1,"endColumn":13},"no-unused-vars","'ReactWaterMark' is assigned a value but never used.","Identifier","unusedVar","'text' is assigned a value but never used.","'options' is assigned a value but never used.","'React